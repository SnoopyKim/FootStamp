<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_area"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/west"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
    <ImageButton
        android:id="@+id/btn_back"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_baseline_chevron_left_24"
        android:scaleType="centerCrop"
        android:background="#00000000"
        app:tint="@color/purple_500"
        android:elevation="1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

        <ImageView
            android:id="@+id/iv_west"
            android:layout_width="400dp"
            android:layout_height="400dp"
            android:src="@drawable/west_all"
            app:tint="@color/purple_500"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <FrameLayout
            android:id="@+id/west_0"
            android:layout_width="157dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="192dp"
            android:layout_height="106dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteX="36dp"
            tools:layout_editor_absoluteY="192dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="강서구"
                android:textStyle="bold"
                android:textColor="@color/purple_500"/>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/west_3"
            android:layout_width="101dp"
            android:layout_height="68dp"
            android:layout_marginStart="133dp"
            android:layout_marginTop="312dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteX="133dp"
            tools:layout_editor_absoluteY="312dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="양천구"
                android:textStyle="bold"
                android:textColor="@color/purple_500"/>

        </FrameLayout>

        <FrameLayout
            android:id="@+id/west_4"
            android:layout_width="88dp"
            android:layout_height="114dp"
            android:layout_marginStart="255dp"
            android:layout_marginTop="297dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteX="255dp"
            tools:layout_editor_absoluteY="297dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="영등포구"
                android:textColor="@color/purple_500"
                android:textStyle="bold" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/west_1"
            android:layout_width="145dp"
            android:layout_height="66dp"
            android:layout_marginStart="97dp"
            android:layout_marginTop="382dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteX="97dp"
            tools:layout_editor_absoluteY="382dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="구로구"
                android:textColor="@color/purple_500"
                android:textStyle="bold" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/west_2"
            android:layout_width="77dp"
            android:layout_height="121dp"
            android:layout_marginStart="250dp"
            android:layout_marginTop="439dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteX="250dp"
            tools:layout_editor_absoluteY="439dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="금천구"
                android:textStyle="bold"
                android:textColor="@color/purple_500"/>
        </FrameLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>